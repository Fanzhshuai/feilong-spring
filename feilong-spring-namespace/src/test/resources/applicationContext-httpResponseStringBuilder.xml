<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:feilong="http://www.feilong.com/schema/feilong"

	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd  
    
    http://www.feilong.com/schema/feilong http://www.feilong.com/schema/feilong/feilong.xsd  
    ">

	<feilong:httpResponseStringBuilder   id="httpResponseStringBuilder" uri="https://blog.csdn.net/fighterandknight/article/details/50126329"
		method="get" userName="" password="" proxyAddress="" proxyPort="" connectTimeout="" readTimeout="" contentCharset="">

		<property name="headerMap">
			<map>
				<entry key="Content-Type" value="application/json" />
			</map>
		</property>

	</feilong:httpResponseStringBuilder>
</beans>  